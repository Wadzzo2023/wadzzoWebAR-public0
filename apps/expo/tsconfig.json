{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"],
      "@ui/*": ["../../packages/ui/*"],
      "@app/*": ["../../packages/app/*"],
      "@api/*": ["../../packages/api/*"],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "../../packages/utils/Common.ts"  ]
}
